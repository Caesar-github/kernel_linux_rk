/ {
	compatible = "rockchip,rk3399pro-toybrick-extboard","rockchip,rk3399pro";
	extbrd: extbrd {
		status = "disabled";
		compatible = "toybrick-extboard";
	};

	es7243-sound {
		compatible = "simple-audio-card";
		simple-audio-card,format = "i2s";
		simple-audio-card,mclk-fs = <256>;
		simple-audio-card,name = "rockchip,es7243";
		simple-audio-card,cpu {
		sound-dai = <&i2s0>;
		};
		simple-audio-card,codec {
			sound-dai = <&es7243_13>;
		};
	};
};

&i2c6 {
	status = "okay";
	i2c-scl-rising-time-ns = <300>;
	i2c-scl-falling-time-ns = <15>;
	clock-frequency=<400000>;
	ina219x47c6: ina219@47 {
		compatible = "ina219";
		reg = <0x47>;
	};
};

&i2c7 {
	status = "okay";
	i2c-scl-rising-time-ns = <345>;
	i2c-scl-falling-time-ns = <11>;
	clock-frequency = <400000>;
	es7243_13: es7243@13 {
		status = "okay";
		compatible = "everest,es7243";
		reg = <0x13>;
		#sound-dai-cells = <0>;
		clocks = <&cru SCLK_I2S_8CH_OUT>;
		clock-names = "mclk";
		//pinctrl-names = "default";
		//pinctrl-0 = <&i2s_8ch_mclk>;
	};
};

&spi5 {
	status = "okay";

	flash@0 {
		compatible = "gigadevice,w25q64", "jedec,spi-nor";
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0>;
		m25p,fast-read;
		// The max SCLK of the flash 104/80 MHZ
		spi-max-frequency = <24000000>;
	};
};

&sdio0 {
	status = "disabled";
};
