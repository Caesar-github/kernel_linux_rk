#include "rk3399pro-toybrick.dtsi"
#include "rk3399-vop-clk-set.dtsi"

/{
	compatible = "rockchip,rk3399pro-toybrick-prop","rockchip,rk3399pro";

	vcc_4g: vcc-4g-regulator {
		compatible = "regulator-fixed";
		enable-active-high;
		gpio = <&gpio2 28 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&vcc_4g_drv>;
		regulator-name = "vcc_4g";
		regulator-always-on;
		regulator-boot-on;
	};

	vcc_pcie: vcc-pcie-regulator {
		compatible = "regulator-fixed";
		enable-active-high;
		gpio = <&gpio2 6 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&vcc_pcie_drv>;
		regulator-name = "vcc_pcie";
		regulator-always-on;
		regulator-boot-on;
	};

	vcc5v0_host: vcc5v0-host-regulator {
		compatible = "regulator-fixed";
		enable-active-high;
		//gpio = <&gpio2 2 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&vcc5v0_host_en>;
		regulator-name = "vcc5v0_host";
		regulator-always-on;
		vin-supply = <&vcc5v0_usb>;
	};

	sdio_pwrseq: sdio-pwrseq {
		compatible = "mmc-pwrseq-simple";
		clocks = <&rk809 1>;
		clock-names = "ext_clock";
		status = "disabled";
	};

	wireless_wlan: wireless-wlan {
		compatible = "wlan-platdata";
		wifi_chip_type = "rtl8188eu";
		status = "okay";
	};

    usbhub_reset: usbhub-reset {
		compatible = "usbhub-reset";
		uhrst-gpio = <&gpio4 21 GPIO_ACTIVE_HIGH>;
    };

	leds: gpio-leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 =<&leds_gpio>;

		led@1 {
			gpios = <&gpio2 RK_PA5 GPIO_ACTIVE_HIGH>;
			label = "system_work_led1"; // Blue LED
			retain-state-suspended;
		};

		led@2 {
			gpios = <&gpio2 RK_PA4 GPIO_ACTIVE_HIGH>;
			label = "system_work_led2"; // Red LED
			retain-state-suspended;
		};

		led@3 {
			gpios = <&gpio2 RK_PA3 GPIO_ACTIVE_HIGH>;
			label = "system_work_led3"; // Green LED
			retain-state-suspended;
		};
	};

	rk_headset: rk-headset {
		compatible = "rockchip_headset";
		headset_gpio = <&gpio0 RK_PB5 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&hp_det>;
		io-channels = <&saradc 3>;
	};
};

&gmac {
	phy-supply = <&vcc_phy>;
	phy-mode = "rgmii";
	clock_in_out = "input";
	snps,reset-gpio = <&gpio3 RK_PC0 GPIO_ACTIVE_LOW>;
	snps,reset-active-low;
	snps,reset-delays-us = <0 10000 50000>;
	assigned-clocks = <&cru SCLK_RMII_SRC>;
	assigned-clock-parents = <&clkin_gmac>;
	pinctrl-names = "default";
	pinctrl-0 = <&rgmii_pins>;
	tx_delay = <0x28>;
	rx_delay = <0x20>;
	status = "okay";
};

&i2c4 {
	status = "okay";
	gsl3673: gsl3673@40 {
		status = "disabled";
		compatible = "GSL,GSL3673";
		pinctrl-names = "default";
		pinctrl-0 = <&edp_touch_int>;
		reg = <0x40>;
		screen_max_x = <1536>;
		screen_max_y = <2048>;

		irq_gpio_number = <&gpio4 RK_PD0 IRQ_TYPE_LEVEL_LOW>;
		rst_gpio_number = <&gpio4 RK_PC6 GPIO_ACTIVE_HIGH>;
	};

	gt9xx: gt9xx@14 {
		stauts = "disabled";
		compatible = "goodix,gt9xx";
		reg = <0x14>;
		touch-gpio = <&gpio2 RK_PB4 IRQ_TYPE_LEVEL_LOW>;
		reset-gpio = <&gpio2 RK_PA2 GPIO_ACTIVE_LOW>;
		max-x = <1920>;
		max-y = <1080>;
		tp-size = <928>;
	};
};

&i2c8 {
	status = "okay";
	i2c-scl-rising-time-ns = <345>;
	i2c-scl-falling-time-ns = <11>;
	clock-frequency = <100000>;

	fusb0: fusb30x@22 {
		compatible = "fairchild,fusb302";
		reg = <0x22>;
		pinctrl-names = "default";
		pinctrl-0 = <&fusb0_int>;
		int-n-gpios = <&gpio1 RK_PA2 GPIO_ACTIVE_HIGH>;
		vbus-5v-gpios = <&gpio0 RK_PA1 GPIO_ACTIVE_LOW>;
		status = "okay";
	};
};

&usbdrd3_0 {
	status = "okay";
};

&usbdrd3_1 {
	status = "okay";
};

&usbdrd_dwc3_0 {
    status = "okay";
};


&usbdrd_dwc3_1 {
    snps,dis-u3-autosuspend-quirk;
	status = "okay";
};

&tcphy0 {
	extcon = <&fusb0>;
	status = "okay";
};

&tcphy1 {
	status = "okay";
};

&u2phy0 {
	status = "okay";
	extcon = <&fusb0>;

	u2phy0_otg: otg-port {
		status = "okay";
	};

	u2phy0_host: host-port {
		phy-supply = <&vcc5v0_host>;
		status = "okay";
	};
};

&u2phy1 {
	status = "okay";

	u2phy1_otg: otg-port {
		status = "okay";
	};

	u2phy1_host: host-port {
		phy-supply = <&vcc5v0_host>;
		status = "okay";
	};
};

&cdn_dp {
	status = "disabled";
	extcon = <&fusb0>;
	phys = <&tcphy0_dp>;
};

&dp_in_vopb {
	status = "disabled";
};

&io_domains {
	status = "okay";
	bt656-supply = <&vcca_1v8>; /* APIO2_VDD */
	audio-supply = <&vcca_1v8>; /* APIO5_VDD */
	sdmmc-supply = <&vccio_sd>; /* SDMMC0_VDD */
	gpio1830-supply = <&vcc_3v0>; /* APIO4_VDD */
};

&pmu_io_domains {
	status = "okay";
	pmu1830-supply = <&vcc_1v8>;
};

&uart4 {
	status = "disabled";
};

&pwm3 {
	status = "okay";
	pinctrl-names = "default";
	interrupts = <GIC_SPI 61 IRQ_TYPE_LEVEL_HIGH 0>;
	compatible = "rockchip,remotectl-pwm";
	remote_pwm_id = <3>;
	handle_cpu_id = <1>;

	ir_key1 {
		rockchip,usercode = <0x4040>;
		rockchip,key_table =
			<0xf2	KEY_REPLY>,
			<0xba	KEY_BACK>,
			<0xf4	KEY_UP>,
			<0xf1	KEY_DOWN>,
			<0xef	KEY_LEFT>,
			<0xee	KEY_RIGHT>,
			<0xbd	KEY_HOME>,
			<0xea	KEY_VOLUMEUP>,
			<0xe3	KEY_VOLUMEDOWN>,
			<0xe2	KEY_SEARCH>,
			<0xb2	KEY_POWER>,
			<0xbc	KEY_MUTE>,
			<0xec	KEY_MENU>,
			<0xbf	0x190>,
			<0xe0	0x191>,
			<0xe1	0x192>,
			<0xe9	183>,
			<0xe6	248>,
			<0xe8	185>,
			<0xe7	186>,
			<0xf0	388>,
			<0xbe	0x175>;
	};

	ir_key2 {
		rockchip,usercode = <0xff00>;
		rockchip,key_table =
			<0xf9	KEY_HOME>,
			<0xbf	KEY_BACK>,
			<0xfb	KEY_MENU>,
			<0xaa	KEY_REPLY>,
			<0xb9	KEY_UP>,
			<0xe9	KEY_DOWN>,
			<0xb8	KEY_LEFT>,
			<0xea	KEY_RIGHT>,
			<0xeb	KEY_VOLUMEDOWN>,
			<0xef	KEY_VOLUMEUP>,
			<0xf7	KEY_MUTE>,
			<0xe7	KEY_POWER>,
			<0xfc	KEY_POWER>,
			<0xa9	KEY_VOLUMEDOWN>,
			<0xa8	KEY_VOLUMEDOWN>,
			<0xe0	KEY_VOLUMEDOWN>,
			<0xa5	KEY_VOLUMEDOWN>,
			<0xab	183>,
			<0xb7	388>,
			<0xe8	388>,
			<0xf8	184>,
			<0xaf	185>,
			<0xed	KEY_VOLUMEDOWN>,
			<0xee	186>,
			<0xb3	KEY_VOLUMEDOWN>,
			<0xf1	KEY_VOLUMEDOWN>,
			<0xf2	KEY_VOLUMEDOWN>,
			<0xf3	KEY_SEARCH>,
			<0xb4	KEY_VOLUMEDOWN>,
			<0xbe	KEY_SEARCH>;
	};

	ir_key3 {
		rockchip,usercode = <0x1dcc>;
		rockchip,key_table =
			<0xee	KEY_REPLY>,
			<0xf0	KEY_BACK>,
			<0xf8	KEY_UP>,
			<0xbb	KEY_DOWN>,
			<0xef	KEY_LEFT>,
			<0xed	KEY_RIGHT>,
			<0xfc	KEY_HOME>,
			<0xf1	KEY_VOLUMEUP>,
			<0xfd	KEY_VOLUMEDOWN>,
			<0xb7	KEY_SEARCH>,
			<0xff	KEY_POWER>,
			<0xf3	KEY_MUTE>,
			<0xbf	KEY_MENU>,
			<0xf9	0x191>,
			<0xf5	0x192>,
			<0xb3	388>,
			<0xbe	KEY_1>,
			<0xba	KEY_2>,
			<0xb2	KEY_3>,
			<0xbd	KEY_4>,
			<0xf9	KEY_5>,
			<0xb1	KEY_6>,
			<0xfc	KEY_7>,
			<0xf8	KEY_8>,
			<0xb0	KEY_9>,
			<0xb6	KEY_0>,
			<0xb5	KEY_BACKSPACE>;
	};
};

&pinctrl {

    vcc_4g_drv: vcc-4g-drv {
		rockchip,pins =
			<2 RK_PD4 RK_FUNC_GPIO &pcfg_pull_up>, // 3G_PWR_EN
			<2 RK_PD2 RK_FUNC_GPIO &pcfg_output_low>, // 4G_ENABLE
			<2 RK_PD3 RK_FUNC_GPIO &pcfg_output_low>; // 4G_RESET
	};

	fusb30x {
		fusb0_int: fusb0-int {
			rockchip,pins =
				<1 RK_PA2 0 &pcfg_pull_up>;
		};
	};

	usb3 {
		vcc5v0_host_en: vcc5v0-host-en {
			rockchip,pins =
				<2 RK_PA2 RK_FUNC_GPIO &pcfg_output_high>;
		};
	};

	leds_gpio: leds-gpio {
		rockchip,pins =
			<2 RK_PA5 RK_FUNC_GPIO &pcfg_pull_up>,
			<2 RK_PA4 RK_FUNC_GPIO &pcfg_pull_up>,
			<2 RK_PA3 RK_FUNC_GPIO &pcfg_pull_up>;
	};

	headphone {
		hp_det: hp-det {
			rockchip,pins =
				<0 RK_PB5 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

    pcie {
		vcc_pcie_drv: vcc-pcie-drv {
			rockchip,pins =
				<2 RK_PA6 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	touch {
		edp_touch_int: edp-touch-int {
			rockchip,pins =
				<4 RK_PD0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	lcd {
		edp_pwren_h: edp-pwren-h {
			rockchip,pins =
				<4 RK_PD4 RK_FUNC_GPIO &pcfg_output_high>;
		};
	};
};


