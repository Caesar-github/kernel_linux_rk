/dts-v1/;
#include "rk3399pro-toybrick-prox.dtsi"
#include "rk3399-android.dtsi"
#include "rk3399pro-toybrick-prox-isp1-virtual-tc35874x-ctl-by-3399.dtsi"
#include "rk3399pro-toybrick-prox-isp1-imx317.dtsi"
#include "rk3399pro-toybrick-edp-1080p-hdmi.dtsi"

/ {
	compatible = "rockchip,rk3399pro-toybrick-prox-android","rockchip,rk3399pro";

	chosen {
		bootargs = "earlycon=uart8250,mmio32,0xff1a0000 swiotlb=1 console=ttyFIQ0 androidboot.baseband=N/A androidboot.selinux=permissive androidboot.hardware=rk30board androidboot.console=ttyFIQ0 init=/init initrd=0x62000001,0x00800000 coherent_pool=1m";
	};

	rt5672-sound {
		compatible = "rockchip-rt5670";
		status = "okay";
		dais {
			dai0 {
				audio-codec = <&rt5670>;
				audio-controller = <&i2s0>;
				format = "i2s";
			};
			dai1 {
				audio-codec = <&rt5670>;
				audio-controller = <&i2s0>;
				format = "i2s";
			};
			dai2 {
				audio-codec = <&es7210>;
				audio-controller = <&i2s0>;
				format = "i2s";
			};
		};
	};

	adc-keys {
		compatible = "adc-keys";
		io-channels = <&saradc 2>;
		io-channel-names = "buttons";
		poll-interval = <100>;
		keyup-threshold-microvolt = <1800000>;

		menu-key {
			linux,code = <KEY_MENU>;
			label = "menu";
			press-threshold-microvolt = <1250000>;
		};

		mute-key {
			linux,code = <KEY_MUTE>;
			label = "mute";
			press-threshold-microvolt = <850000>;
		};

		vol-down-key {
			linux,code = <KEY_VOLUMEDOWN>;
			label = "volume down";
			press-threshold-microvolt = <400000>;
		};

		vol-up-key {
			linux,code = <KEY_VOLUMEUP>;
			label = "volume up";
			press-threshold-microvolt = <20000>;
		};
	};
};

&tcphy0 {
	rockchip,force_usb2 = <1>;
};

&usbdrd_dwc3_0 {
	maximum-speed = "high-speed";
};

&rk_headset {
	status = "disabled";
};

&i2c1 {
	status = "okay";
	i2c-scl-rising-time-ns = <345>;
	i2c-scl-falling-time-ns = <11>;
	clock-frequency = <400000>;
	rt5670: rt5670@1c {
		#sound-dai-cell = <0>;
		compatible = "realtek,rt5670";
		reg = <0x1c>;
		status = "okay";
	};
};

&i2c6 {
	status = "okay";
	es7210: es7210@40 {
		#sound-dai-cells = <0>;
		compatible = "MicArray_0";
		reg = <0x40>;
		clocks = <&cru SCLK_I2S_8CH_OUT>;
		clock-names = "mclk";
		pinctrl-names = "default";
		pinctrl-0 = <&i2s_8ch_mclk>;
	};

	es7210_1: es7210@42 {
		compatible = "MicArray_1";
		reg = <0x42>;
	};

	sensor@77 {
		status = "okay";
		compatible = "gs_toybrick";
		reg = <0x77>;
		type = <SENSOR_TYPE_ACCEL>;
		reprobe_en = <1>;
		irq_enable = <0>;
		poll_delay_ms = <30>;
	};
};

&rk809_sound {
	status = "disabled";
};

&i2s0 {
       status = "okay";
       dmas = <&dmac_bus 0>, <&dmac_bus 1>;
       dma-names = "tx", "rx";
       #sound-dai-cells = <0>;
       clock-names = "i2s_clk", "i2s_mclk","i2s_hclk";
       clocks = <&cru SCLK_I2S0_8CH>, <&cru SCLK_I2S_8CH_OUT>, <&cru HCLK_I2S0_8CH>;
       pinctrl-names = "default";
       pinctrl-0 = <&i2s0_8ch_bus>;
	   rockchip,playback-channels = <8>;
	   rockchip,capture-channels = <8>;
};

&dp_sound {
	status = "disabled";
};

&hdmi_dp_sound {
	status = "disabled";
};

&hdmi_sound {
	status = "okay";
};

&i2c1 {
	status = "okay";
	i2c-scl-rising-time-ns = <300>;
	i2c-scl-falling-time-ns = <15>;
	clock-frequency=<400000>;
};

&i2c2 {
	status = "okay";
	i2c-scl-rising-time-ns = <300>;
	i2c-scl-falling-time-ns = <15>;
	clock-frequency=<400000>;
};

&isp0_mmu {
	status = "okay";
};

&isp1_mmu {
	status = "okay";
};

&route_hdmi {
	status = "disabled";
};

&route_dsi {
	status = "disabled";
};

&leds {
	status = "disabled";
};

&sdmmc {
	status = "disabled";
};


&pcie0 {
	status = "okay";
	max-link-speed = <2>;
	ep-gpios = <&gpio1 RK_PB1 GPIO_ACTIVE_HIGH>;
};

&sdio_pwrseq {
	status = "disabled";
};

&wireless_wlan {
	status = "okay";
	clocks = <&hym8563>;
	clock-names = "ext_clk";
	wifi_chip_type = "ap6275";
	WIFI,reset_gpio = <&gpio2 RK_PD3 GPIO_ACTIVE_HIGH>;
};

&pwm1 {
	status = "okay";
	interrupts = <GIC_SPI 61 IRQ_TYPE_LEVEL_HIGH 0>;
	compatible = "rockchip,remotectl-pwm";
	remote_pwm_id = <1>;
	handle_cpu_id = <1>;
	remote_support_psci = <0>;

	pinctrl-names = "default";
	pinctrl-0 = <&pwm1_pin>;

	ir_key1 {
		rockchip,usercode = <0x4040>;
		rockchip,key_table =
			<0xf2	KEY_REPLY>,
			<0xba	KEY_BACK>,
			<0xf4	KEY_UP>,
			<0xf1	KEY_DOWN>,
			<0xef	KEY_LEFT>,
			<0xee	KEY_RIGHT>,
			<0xbd	KEY_HOME>,
			<0xea	KEY_VOLUMEUP>,
			<0xe3	KEY_VOLUMEDOWN>,
			<0xe2	KEY_SEARCH>,
			<0xb2	KEY_POWER>,
			<0xbc	KEY_MUTE>,
			<0xec	KEY_MENU>,
			<0xbf	0x190>,
			<0xe0	0x191>,
			<0xe1	0x192>,
			<0xe9	183>,
			<0xe6	248>,
			<0xe8	185>,
			<0xe7	186>,
			<0xf0	388>,
			<0xbe	0x175>;
	};

	ir_key2 {
		rockchip,usercode = <0xff00>;
		rockchip,key_table =
			<0xf9	KEY_HOME>,
			<0xbf	KEY_BACK>,
			<0xfb	KEY_MENU>,
			<0xaa	KEY_ENTER>,
			<0xb9	KEY_UP>,
			<0xe9	KEY_DOWN>,
			<0xb8	KEY_LEFT>,
			<0xea	KEY_RIGHT>,
			<0xeb	KEY_VOLUMEDOWN>,
			<0xef	KEY_VOLUMEUP>,
			<0xf7	KEY_MUTE>,
			<0xe7	KEY_POWER>,
			<0xfc	KEY_POWER>,
			<0xa9	KEY_VOLUMEDOWN>,
			<0xa8	KEY_VOLUMEDOWN>,
			<0xe0	KEY_VOLUMEDOWN>,
			<0xa5	KEY_VOLUMEDOWN>,
			<0xab	183>,
			<0xb7	388>,
			<0xf8	184>,
			<0xaf	185>,
			<0xed	KEY_VOLUMEDOWN>,
			<0xee	186>,
			<0xb3	KEY_VOLUMEDOWN>,
			<0xf1	KEY_VOLUMEDOWN>,
			<0xf2	KEY_VOLUMEDOWN>,
			<0xf3	KEY_SEARCH>,
			<0xb4	KEY_VOLUMEDOWN>,
			<0xbe	KEY_SEARCH>;
	};

	ir_key3 {
		rockchip,usercode = <0x1dcc>;
		rockchip,key_table =
			<0xee	KEY_REPLY>,
			<0xf0	KEY_BACK>,
			<0xf8	KEY_UP>,
			<0xbb	KEY_DOWN>,
			<0xef	KEY_LEFT>,
			<0xed	KEY_RIGHT>,
			<0xfc	KEY_HOME>,
			<0xf1	KEY_VOLUMEUP>,
			<0xfd	KEY_VOLUMEDOWN>,
			<0xb7	KEY_SEARCH>,
			<0xff	KEY_POWER>,
			<0xf3	KEY_MUTE>,
			<0xbf	KEY_MENU>,
			<0xf9	0x191>,
			<0xf5	0x192>,
			<0xb3	388>,
			<0xbe	KEY_1>,
			<0xba	KEY_2>,
			<0xb2	KEY_3>,
			<0xbd	KEY_4>,
			<0xf9	KEY_5>,
			<0xb1	KEY_6>,
			<0xfc	KEY_7>,
			<0xf8	KEY_8>,
			<0xb0	KEY_9>,
			<0xb6	KEY_0>,
			<0xb5	KEY_BACKSPACE>;
	};
};

&pinctrl {
	usb3 {
		vcc5v0_host_en: vcc5v0-host-en {
			rockchip,pins =
				<2 RK_PA3 RK_FUNC_GPIO &pcfg_output_high>;
		};
	};

	lt9611 {
		lt9611_pwr: lt9611-pwr {
			rockchip,pins =
				<4 RK_PB3 RK_FUNC_GPIO &pcfg_output_high>;
		};
	};

	pwm1 {
		pwm1_pin: pwm1-pin {
			rockchip,pins =
				<4 RK_PC6 RK_FUNC_1 &pcfg_pull_none>;
		};
	};
};
